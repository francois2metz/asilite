<ol class="breadcrumb">
  <li><a href="<%= page_path(@conn, :index) %>">Accueil</a></li>
  <li class="active"><%= @type %></li>
</ol>
<%= for hit <- @results["hits"]["hits"] do %>
  <div>
    <a href="<%= hit["path"] %>">
      <h1><%= hit["title"] %></h1>
      <p><%= hit["tease"] %></p>
    </a>
  </div>
<% end %>
